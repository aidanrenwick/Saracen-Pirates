	(game-time > 300)
	(difficulty == moderate)
	(unit-type-count-total 1795 < 4)
	(can-train 1795)
=>
	(train 1795)
	(chat-local-to-self "Dromon")
)

(defrule
	(game-time > 200)
	(difficulty == hard)
	(unit-type-count-total 1795 < 6)
