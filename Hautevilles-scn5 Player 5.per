=>
	(train 1795)
	(chat-local-to-self "Dromon")
)





(defrule
	(event-detected trigger 50)
=>
	(chat-local-to-self "Player is my enemy but not a threat. Go into casual mode.")
	(set-goal current-phase 2)
	(acknowledge-event trigger 50)
)

(defrule
	(event-detected trigger 51)
	(not(players-stance 1 ally))
=>
	(chat-local-to-self "Player has built a wonder. Go into super aggressive mode.")
	(set-goal current-phase 3)
	(acknowledge-event trigger 51)
